{
  "expo": {
    "name": "portfolioapp",
    "slug": "portfolioapp",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/android-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aleksandrmelnikov.portfolioapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.aleksandrmelnikov.portfolioapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
